package homework_1;

public class MuhendislikHesapMakineImpl implements MuhendislikHesapMakine {
	
	 public void toplama(double a,double b) {
		 	System.out.println(a+b);
		     }
		    public void cikarma(double a,double b) {
		     System.out.println(a-b);
		     }
		    public void carpma(double a,double b) {
		    System.out.println(a*b);
		     }
		    public void bolme(double a,double b) {
		    System.out.println(a/b);
		     }
		    public void usalma(double a,double b) {
			System.out.println(Math.pow(a,b));
			 }
		    public void karekokalma(double a) {
			System.out.println(Math.sqrt(a));
		     }

}

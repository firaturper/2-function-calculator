package homework_1;

public interface MuhendislikHesapMakine {
	
	static double toplama(double a,double b) {
		return a+b;
	}
	
	static double cikarma(double a,double b) {
		return a-b;
	}
	
	static double carpma(double a,double b) {
		return a*b;
	}
	
	static double bolme(double a,double b) {
		return a/b;
	}
	
	static double usalma(double a,double b) {
		return  Math.pow(a,b); 
	}
	
	static double karekokalma(double a) {
		return  Math.sqrt(a); 
	}
	

}
